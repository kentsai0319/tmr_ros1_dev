<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <xacro:include filename="$(find tmr_description)/xacro/macro.inertial.xacro" />

  <xacro:macro name="tm12s" params="ns prefix
    color:=none format:=obj">
<!--
    <xacro:property name="shoulder_offset" value="0.183" />
    <xacro:property name="elbow_offset" value="0.1327" />
    <xacro:property name="wrist_offset" value="0.1315" />
-->
    <xacro:property name="mass_0" value="1.0" />
    <xacro:property name="mass_1" value="8.2" />
    <xacro:property name="mass_2" value="15.02" />
    <xacro:property name="mass_3" value="3.3" />
    <xacro:property name="mass_4" value="1.77" />
    <xacro:property name="mass_5" value="1.77" />
    <xacro:property name="mass_6" value="0.72" />

<!--LinkDescription-->
  <link name="${prefix}link_0">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/visual/tm12s-base.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/collision/tm12s-base.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <sphere radius="0.11" />
      </geometry>
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_z radius="0.08" length="0.05" mass="${mass_0}">
      <origin xyz="0 0 0.025" rpy="0 0 0" />
    </xacro:cylinder_inertial_z>
  </link>

  <joint name="${prefix}joint_1" type="revolute">
    <parent link="${prefix}link_0" />
    <child link="${prefix}link_1" />
    <origin xyz="0 0 0.1652" rpy="0 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-1.5*pi}" upper="${1.5*pi}" velocity="3.14" effort="320.0" />
  </joint>

  <link name="${prefix}link_1">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/visual/tm12s-j1.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/collision/tm12s-j1.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.205" radius="0.085" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.0882" radius="0.085" />
      </geometry>
      <origin xyz="0 -0.0441 0" rpy="${-0.5*pi} 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_z radius="0.075" length="0.16" mass="${mass_1}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_z>
  </link>

  <joint name="${prefix}joint_2" type="revolute">
    <parent link="${prefix}link_1" />
    <child link="${prefix}link_2" />
    <origin xyz="0 0 0" rpy="${-0.5*pi} ${-0.5*pi} 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-pi}" upper="${pi}" velocity="3.14" effort="320" />
  </joint>

  <link name="${prefix}link_2">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/visual/tm12s-a1.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/collision/tm12s-a1.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.205" radius="0.085" />
      </geometry>
      <origin xyz="0 0 -0.183" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.6361" radius="0.076" />
      </geometry>
      <origin xyz="0.31805 0 -0.183" rpy="0 ${0.5*pi} 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.175" radius="0.076" />
      </geometry>
      <origin xyz="0.6361 0 -0.186" rpy="0 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_x radius="0.072" length="0.6361" mass="${mass_2}">
      <origin xyz="0.31805 0 -0.183" rpy="0 0 0" />
    </xacro:cylinder_inertial_x>
  </link>

  <joint name="${prefix}joint_3" type="revolute">
    <parent link="${prefix}link_2" />
    <child link="${prefix}link_3" />
    <origin xyz="0.6361 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-pi}" upper="${pi}" velocity="3.14" effort="320" />
  </joint>

  <link name="${prefix}link_3">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/visual/tm12s-a2.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm12s/collision/tm12s-a2.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.075" />
      </geometry>
      <origin xyz="0 0 -0.0558" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.5324" radius="0.045" />
      </geometry>
      <origin xyz="0.2662 0 -0.0503" rpy="0 ${0.5*pi} 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.136" radius="0.047" />
      </geometry>
      <origin xyz="0.5324 0 -0.0553" rpy="0 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_x radius="0.045" length="0.5324" mass="${mass_3}">
      <origin xyz="0.2662 0 -0.0503" rpy="0 0 0" />
    </xacro:cylinder_inertial_x>
  </link>

  <joint name="${prefix}joint_4" type="revolute">
    <parent link="${prefix}link_3" />
    <child link="${prefix}link_4" />
    <origin xyz="0.5324 0 -0.1818" rpy="0 0 ${0.5*pi}" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-pi}" upper="${pi}" velocity="3.92" effort="80" />
  </joint>

  <link name="${prefix}link_4">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm5s/visual/tm5s-j5.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm5s/collision/tm5s-j5.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.0608" radius="0.047" />
      </geometry>
      <origin xyz="0 0 0.0304" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.136" radius="0.047" />
      </geometry>
      <origin xyz="0 -0.005 0" rpy="${0.5*pi} 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_y radius="0.045" length="0.12" mass="${mass_4}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_y>
  </link>

  <joint name="${prefix}joint_5" type="revolute">
    <parent link="${prefix}link_4" />
    <child link="${prefix}link_5" />
    <origin xyz="0 -0.1315 0" rpy="${0.5*pi} 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-pi}" upper="${pi}" velocity="3.92" effort="80" />
  </joint>

  <link name="${prefix}link_5">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm5s/visual/tm5s-j6.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm5s/collision/tm5s-j6.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.0608" radius="0.047" />
      </geometry>
      <origin xyz="0 0 -0.0304" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.136" radius="0.047" />
      </geometry>
      <origin xyz="0 -0.005 0" rpy="${0.5*pi} 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_y radius="0.045" length="0.12" mass="${mass_5}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_y>
  </link>

  <joint name="${prefix}joint_6" type="revolute">
    <parent link="${prefix}link_5" />
    <child link="${prefix}link_6" />
    <origin xyz="0 -0.13495 0" rpy="${0.5*pi} 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-1.5*pi}" upper="${1.5*pi}" velocity="3.92" effort="80" />
  </joint>

  <link name="${prefix}link_6">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm5s/visual/tm5s-io.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm5s/collision/tm5s-io.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.062" radius="0.047" />
      </geometry>
      <origin xyz="0 0 -0.031" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <box size="0.076 0.064 0.12" />
      </geometry>
      <origin xyz="0 0.074 -0.008" rpy="0 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_z radius="0.045" length="0.06" mass="${mass_6}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_z>
  </link>

  <link name="${prefix}base" />
  <joint name="${prefix}base_fixed_joint" type="fixed">
    <parent link="${prefix}base" />
    <child link="${prefix}link_0" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>

  <link name="${prefix}flange" />
  <joint name="${prefix}flange_fixed_joint" type="fixed">
    <parent link="${prefix}link_6" />
    <child link="${prefix}flange" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>
<!--LinkDescription-->

  </xacro:macro>

</robot>
