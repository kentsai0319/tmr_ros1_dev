<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <xacro:include filename="$(find tmr_description)/xacro/macro.inertial.xacro" />

  <xacro:macro name="tm25s" params="ns prefix
    color:=none format:=dae">
<!--
    <xacro:property name="shoulder_offset" value="0.253" />
    <xacro:property name="elbow_offset" value="0.183" />
    <xacro:property name="wrist_offset" value="0.1702" />
-->
    <xacro:property name="mass_0" value="1.0" />
    <xacro:property name="mass_1" value="22.67" />
    <xacro:property name="mass_2" value="36.40" />
    <xacro:property name="mass_3" value="8.03" />
    <xacro:property name="mass_4" value="8.04" />
    <xacro:property name="mass_5" value="2.35" />
    <xacro:property name="mass_6" value="0.72" />

<!--LinkDescription-->
  <link name="${prefix}link_0">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/visual/tm25s-base.stl" />
      </geometry>
      <material name="grey" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/collision/tm25s-base.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <sphere radius="0.135" />
      </geometry>
      <origin xyz="0 0 0.01" rpy="0 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_z radius="0.1" length="0.08" mass="${mass_0}">
      <origin xyz="0 0 0.04" rpy="0 0 0" />
    </xacro:cylinder_inertial_z>
  </link>

  <joint name="${prefix}joint_1" type="revolute">
    <parent link="${prefix}link_0" />
    <child link="${prefix}link_1" />
    <origin xyz="0 0 0.235" rpy="0 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-2.0*pi}" upper="${2.0*pi}" velocity="3.14" effort="1000.0" />
  </joint>

  <link name="${prefix}link_1">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/visual/tm25s-j1.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/collision/tm25s-j1.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.3" radius="0.115" />
      </geometry>
      <origin xyz="0 0 0.013" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.118" radius="0.115" />
      </geometry>
      <origin xyz="0 -0.059 0" rpy="${-0.5*pi} 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_z radius="0.06" length="0.16" mass="${mass_1}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_z>
  </link>

  <joint name="${prefix}joint_2" type="revolute">
    <parent link="${prefix}link_1" />
    <child link="${prefix}link_2" />
    <origin xyz="0 0 0" rpy="${-0.5*pi} ${-0.5*pi} 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-2.0*pi}" upper="${2.0*pi}" velocity="3.14" effort="1000" />
  </joint>

  <link name="${prefix}link_2">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/visual/tm25s-a1.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/collision/tm25s-a1.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.3" radius="0.115" />
      </geometry>
      <origin xyz="0 0 -0.266" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.950" radius="0.102" />
      </geometry>
      <origin xyz="0.475 0 -0.253" rpy="0 ${0.5*pi} 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.205" radius="0.085" />
      </geometry>
      <origin xyz="0.950 0 -0.253" rpy="0 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_x radius="0.09" length="0.950" mass="${mass_2}">
      <origin xyz="0.475 0 -0.253" rpy="0 0 0" />
    </xacro:cylinder_inertial_x>
  </link>

  <joint name="${prefix}joint_3" type="revolute">
    <parent link="${prefix}link_2" />
    <child link="${prefix}link_3" />
    <origin xyz="0.950 0 -0.070" rpy="0 0 ${0.5*pi}" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-pi}" upper="${pi}" velocity="3.14" effort="500" />
  </joint>

  <link name="${prefix}link_3">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/visual/tm25s-j4.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/collision/tm25s-j4.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.0882" radius="0.085" />
      </geometry>
      <origin xyz="0 0 -0.0441" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.205" radius="0.085" />
      </geometry>
      <origin xyz="0 0 0" rpy="${0.5*pi} 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_y radius="0.075" length="0.16" mass="${mass_3}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_y>
  </link>

  <joint name="${prefix}joint_4" type="revolute">
    <parent link="${prefix}link_3" />
    <child link="${prefix}link_4" />
    <origin xyz="0 -0.800 0" rpy="${0.5*pi} 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-2.0*pi}" upper="${2.0*pi}" velocity="3.92" effort="320" />
  </joint>

  <link name="${prefix}link_4">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/visual/tm25s-a2.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/collision/tm25s-a2.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.6975" radius="0.075" />
      </geometry>
      <origin xyz="0 0 -0.34875" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.175" radius="0.065" />
      </geometry>
      <origin xyz="0 -0.003 0" rpy="${-0.5*pi} 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_z radius="0.065" length="0.6975" mass="${mass_4}">
      <origin xyz="0 0 -0.34875" rpy="0 0 0" />
    </xacro:cylinder_inertial_z>
  </link>

  <joint name="${prefix}joint_5" type="revolute">
    <parent link="${prefix}link_4" />
    <child link="${prefix}link_5" />
    <origin xyz="0 0.1702 0" rpy="${-0.5*pi} 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-2.0*pi}" upper="${2.0*pi}" velocity="3.92" effort="160" />
  </joint>

  <link name="${prefix}link_5">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/visual/tm25s-j6.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/collision/tm25s-j6.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.093" radius="0.065" />
      </geometry>
      <origin xyz="0 0 -0.0465" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.175" radius="0.058" />
      </geometry>
      <origin xyz="0 -0.0035 0" rpy="${0.5*pi} 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_y radius="0.045" length="0.16" mass="${mass_5}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_y>
  </link>

  <joint name="${prefix}joint_6" type="revolute">
    <parent link="${prefix}link_5" />
    <child link="${prefix}link_6" />
    <origin xyz="0 -0.15295 0" rpy="${0.5*pi} 0 0" />
    <axis xyz="0 0 1" />
<!--limit-->
    <limit lower="${-2.0*pi}" upper="${2.0*pi}" velocity="3.92" effort="160" />
  </joint>

  <link name="${prefix}link_6">
    <visual>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/visual/tm25s-io.${format}" />
      </geometry>
      <material name="${color}" />
    </visual>
    <!--collision>
      <geometry>
        <mesh filename="package://tmr_description/meshes/tm25s/collision/tm25s-io.obj" />
      </geometry>
    </collision-->
    <collision>
      <geometry>
        <cylinder length="0.062" radius="0.047" />
      </geometry>
      <origin xyz="0 0 -0.031" rpy="0 0 0" />
    </collision>
    <collision>
      <geometry>
        <box size="0.076 0.064 0.12" />
      </geometry>
      <origin xyz="0 0.074 -0.008" rpy="0 0 0" />
    </collision>
<!--inertial-->
    <xacro:cylinder_inertial_z radius="0.045" length="0.06" mass="${mass_6}">
      <origin xyz="0 0 0" rpy="0 0 0" />
    </xacro:cylinder_inertial_z>
  </link>

  <link name="${prefix}base" />
  <joint name="${prefix}base_fixed_joint" type="fixed">
    <parent link="${prefix}base" />
    <child link="${prefix}link_0" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>

  <link name="${prefix}flange" />
  <joint name="${prefix}flange_fixed_joint" type="fixed">
    <parent link="${prefix}link_6" />
    <child link="${prefix}flange" />
    <origin xyz="0 0 0" rpy="0 0 0" />
  </joint>
<!--LinkDescription-->

  </xacro:macro>

</robot>
